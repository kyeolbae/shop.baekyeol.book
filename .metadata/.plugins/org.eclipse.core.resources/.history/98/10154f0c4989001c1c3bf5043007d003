		  created () { //초기화================================================================
			    //그리드 초기화
		    
		  }, // created		 
		  methods : {  // ======================================================================================
			//file reset
				
			searchButtonOnClick : function() {
				  
				  var param = {
							svcId:"apisearch",
							strUrl:"/ec/exp/sap/apislip/apisearch",	//전송 url 	
							inDs :  "" ,  
							param : serializeObjectX($('#[[${param.pgm_id}]]')), 
							pCall : this.searchButtonOnClickCallBack, 
							pLoad : true          
						};
						//트랜젝션 실행
						ajaxReadJsonData(param);
			  	
			  },
			  
			  searchButtonOnClickCallBack : function(s ,data) {
				  
				  if(data["ds_master"]['data'].RESULT == "ERROR"){
            		  customAlert('','','조회할 수 없습니다.');
            		  return null
				  }
				  
				  this.T_HEADER = [];
				  this.T_VAT = [];
				  this.T_ITEM = [];
				  
				  this.T_HEADER = data["ds_master"]['data'].T_HEADER;
				  this.T_VAT = data["ds_master"]['data'].T_VAT;
				  var ref = this;
				  data["ds_master"]['data'].T_ITEM.forEach(item => ref.T_ITEM.push(item));

				  
				  this.showRealReport();
				  
			  },  
			  showRealReport : 
				  function(){
				  
					  var iframe = document.getElementById(this.pgmid+'realreport');
					       RealReport.openFrame(iframe, this.reportJson, ['T_HEADER', 'T_VAT', 'T_ITEM'], [this.T_HEADER, this.T_VAT, this.T_ITEM]
			  )},
				  
// 			  printPopup :  
// 				  function(){
// 					  var container = document.getElementById('realreport');
